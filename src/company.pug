doctype html
html(lang="ru")
	include modules/head	

	body
		#cursor
		.wrapper 
			include modules/header

			main.main-section
				.page-caption.indent-block__full
					.container 
						h1 Агентство интернет-маркетинга.
						p Мы помогаем бизнесу. 
				.company 
					.company-image.wow.animate__fadeInUp.animated(data-wow-duration="1s",  animation-timing-function="ease-in") 
						img(src="assets/images/company.png", alt="")
					.container
						include modules/cooperation
						include modules/company-odds
						.company-images.indent-block__full.wow.animate__fadeInUp.animated(data-wow-duration="1s",  animation-timing-function="ease-in") 
							.company-images__item 
								img(src="assets/images/company-1.png", alt="")
							.company-images__item 
								img(src="assets/images/company-2.png", alt="")
					include modules/services-list
					include modules/clients
					.container
						include modules/reviews
					include modules/faq
						
			include modules/footer

